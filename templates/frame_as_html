  <head>
    <title tal:content="string:boo"></title>
  </head>

  <body>

<div metal:fill-slot="main">

<h1 tal:content="python:get_frame_pretty_name(here) or get_frame_name(here)"></h1>

<dl tal:define="slots python:get_slot_values(here,'slot_display_order',slot_type=ALL)[0] 
or get_frame_slots(here,slot_type=ALL)[0]"
    tal:condition="slots">
  <span tal:repeat="slot slots"
        tal:omit-tag="">
  <dt><em><a href=""
         tal:attributes="href slot"
         tal:content="python:get_frame_pretty_name(slot) or get_frame_name(slot)"></a></em></dt>
  <dd>
    <span tal:repeat="val python:get_slot_values(here,slot)[0]"
          tal:omit-tag="">
      <span tal:define="show_url python:frame_in_kb_p(val) or len(str(val)) > 4 and str(val)[:4] == 'http'"
            tal:omit-tag="">
	<span tal:condition="python:not show_url" 
              tal:replace="structure val">
        </span>
        <span tal:condition="python:show_url"
              tal:omit-tag="">
          <a href=""
             tal:attributes="href val"
             tal:content="val"></a><br/>
        </span>
      </span>
    </span>
  </dd>
  </span>
</dl>

